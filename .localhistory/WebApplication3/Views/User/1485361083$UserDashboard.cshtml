@model WebApplication3.Models.ViewModel.UserSavedPolygonsView
@{
    ViewBag.Title = "UserDashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserDashboard</h2>

<div class="row">
    
    <div class="col-md-2" >
        <div class="panel panel-default">
            <div class="row">
                <div class="col-xs-12">
                    <a href="#" class="thumbnail"><img data-src="holder.js/100%x180" alt="100%x180" src="data:image/svg+xml;base64," data-holder-rendered="true" style="height: 180px; width: 110%; display: block;"/></a>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-8">
        <div class="panel panel-default">
          <div class="panel-heading">Geojson Polygons</div>
          <div class="panel-body">
              @foreach (var i in Model.UserPolygons)
              {
                    <div class="row polygon-row-dashboard" >
                        <div class="col-xs-3"></div>
                        <div class="col-xs-4">@Html.DisplayFor(m => i.GeoJsonId)</div>
                        <div class="col-xs-5 polygon-operations">
                            <a href="undefined/landscape/homeownermng" data-http-method="delete" class="http-action-link"> 
                            <span class="fa fa-remove fa-lg"></span>&nbsp; Delete</a>

                           <a href="undefined/landscape/homeownermng"> <span class="fa fa-map-o fa-lg"></span>&nbsp; Edit</a>

                           
                      </div>
                  </div>
              }
          </div>
        </div>
    </div>
</div>

